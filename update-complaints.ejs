    <div class="Header-Image">

    </div>
    <div class="content">
        <h1>


            <table>
                <tr>
                    <td><img src="https://i.pinimg.com/474x/bd/43/7f/bd437fe8e7510e0d028bc4cb36f59bb3.jpg" alt=""></td>
                    <td>F.I.R. ENTRY</td>
                </tr>
            </table>


        </h1>

        <h2>Update Complaint</h2>
        <h3><u>Personal Details</u></h3>

        <form action="" method="POST" >
            <table>
                <tr>
                    <div class="name">
                        <input type="hidden" name="id" value="<%= complaints[0].id %> " >
                        Name :
                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="name" name="name" id=""  value="<%= complaints[0].name %> " placeholder="Enter name" required>
                    </div>
                </tr>
                <tr class="content-columns">
                    <td class="content-column-one">
                        <ul>
                            <li> Gender :
                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                                    type="gender" name="gender" id=""  value="<%= complaints[0].gender %> " placeholder="Enter gender" required></li>
                            <li> Current Address : &nbsp;&nbsp;<input type="address1" name="address1" id=""  value="<%= complaints[0].address1 %> "
                                    placeholder="Enter current address" required></li>
                            <li> Place
                                :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                <input type="place" name="place" id="" value="<%= complaints[0].place %> " placeholder="Enter place" required></li>
                            <li>Phone no.
                                :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                                    type="phone" name="phone" id="" value="<%= complaints[0].phone %> " placeholder="Enter Phone no.1" pattern="[0-9]{10}" required></li>
                            <li> Email
                                :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                                    type="email" name="email" id=""  value="<%= complaints[0].email %> " placeholder="Enter Email" required></li>
                            <li> Adhaar No. :&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="adhaar"
                                    name="adhaar" id=""  value="<%= complaints[0].adhaar %> " placeholder="Enter adhaar no." pattern="[0-9]{12}" required></li>




                        </ul>
                    </td>

                    <td class="content-column-two">
                        <ul>
                            <li>Age
                                :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                                    type="number" name="age" id=""  value="<%= complaints[0].age %> " placeholder="Enter age" pattern="[0-9]{2}" required></li>
                            <li> Permanent Address :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="address2"
                                    name="address2" id="" value="<%= complaints[0].address2 %> " placeholder="Enter permanent address" required></li>
                            <li> Pin No.
                                :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                                    type="pin" name="pin" id=""  value="<%= complaints[0].pin %> " placeholder="Enter pin" pattern="[0-9]{6}" required></li>
                            <li> Phone No.
                                :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                                    type="mobile" name="mobile" id=""  value="<%= complaints[0].mobile %> " placeholder="Enter Phone no.2 (Optional)" pattern="[0-9]{10}" ></li>
                            <li> Country
                                :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                                    type="text" value="<%= complaints[0].country %> "  placeholder="Enter Country " required></li>
                            <li> Pan No. : &nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="pan" name="pan" id="" value="<%= complaints[0].pan %> "
                                    placeholder="Enter pan no." pattern="[0-9,a-z]{10}"required></li>


                        </ul>
                    </td>
                </tr>
            </table>
            <div class="complaint-details">


                <h3><u>Complaint details</u></h3>
                <br>
              

                <br>
                <div class="name">
                    Title
                    :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
                        type="title" name="title" id=""  value="<%= complaints[0].title %> "  placeholder="Complaint Title"><br>

                </div>
                <br>
                <div class="name">
                    COMPLAINT :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<textarea name="complaint" id="" cols="80" rows="6"
                    value="<%= complaints[0].complaint %> "    placeholder="Enter Complaint"></textarea>
                </div>








            </div>


            <br>
    </div>

    <div class="submit">
        <button value="submit">Update</button>
    </div>

    </form>
    <div class="footer-above">
        <table>
            <tr>
                <td>
                    <ul>
                        <h3>F.I.R.Submission</h3>

